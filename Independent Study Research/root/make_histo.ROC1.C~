TH1F * ha2 = new TH1F("ha2","adc 2 histogram", 1200, 0., 1200.);
TH1F * ha3 = new TH1F("ha3","adc 3 histogram", 1200, 0., 1200.);
TH1F * ha4 = new TH1F("ha4","adc 4 histogram", 1200, 0., 1200.);
TH1F * ha5 = new TH1F("ha5","adc 5 histogram", 1200, 0., 1200.);
TH1F * ha6 = new TH1F("ha6","adc 6 histogram", 1200, 0., 1200.);
TH1F * ha7 = new TH1F("ha7","adc 7 histogram", 1200, 0., 1200.);
camac->Project("ha0","adc0");
camac->Project("ha1","adc1");
camac->Project("ha2","adc2");
camac->Project("ha3","adc3");
camac->Project("ha4","adc4");
camac->Project("ha5","adc5");
camac->Project("ha6","adc6");
camac->Project("ha7","adc7");
TH1F * ht0 = new TH1F("ht0","tdc 0 histogram", 2400, 0., 2400.);
